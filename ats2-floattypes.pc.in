prefix=@prefix@
exec_prefix=@exec_prefix@
libdir=@libdir@
includedir=@includedir@
host=@host@
package=@PACKAGE@
version=@VERSION@

PATSOPTFLAGS="-IATS ${includedir}/${package}/${host} -IATS ${includedir}/${package}"
PATSCCFLAGS="-IIATS ${includedir}/${package}/${host} -IIATS ${includedir}/${package}"

Name: ${package}
Description: More floating-point support for ATS2/Postiats
Version: ${version}
Libs: -L${libdir} -lats2-floattypes -lm
Cflags: @CPPFLAGS@ -I${includedir}/${package}/${host} -I${includedir}/${package}
